{
	"name": "binary-viewer",
	"displayName": "Binary Viewer",
	"author": {
		"name": "QiaoJie",
		"email": "qiao.jie@outlook.com"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/qiaojie/BinaryViewer/issues"
	},
	"publisher": "QiaoJie",
	"icon": "images/icon.png",
	"description": "binary viewer for vscode",
	"version": "1.1.1",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.binaryView"
	],
	"keywords": [
		"binary",
		"Hexadecimal",
		"viewer"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.binaryView",
				"title": "Open As Hexadecimal View",
				"icon": {
					"light": "images/icon-light.png",
					"dark": "images/icon-dark.png"
				}
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "extension.binaryView",
					"group": "navigation"
				}
			],
			"editor/title": [
				{
					"command": "extension.binaryView",
					"group": "navigation"
				}
			],
			"editor/title/context": [
				{
					"command": "extension.binaryView",
					"group": "navigation"
				}
			],
			"explorer/context": [
				{
					"command": "extension.binaryView",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/qiaojie/BinaryViewer"
	},
	"devDependencies": {
		"@types/node": "^13.13.5",
		"@types/vscode": "^1.44.0",
		"typescript": "^3.8.3"
	},
	"__metadata": {
		"id": "11bab443-6426-4243-bb3b-f6e673501ac3",
